extends ../layout

block content
    h1= title

    ul
        - model_list.sort(function(a, b) {let textA = a.name.toUpperCase(); let textB = b.name.toUpperCase(); return (textA < textB) ? -1 : (textA > textB) ? 1 : 0;});
        each model in model_list
            li
                a(href=model.url) #{model.name} - #{model.brand.name}
        else
            li There are no models.